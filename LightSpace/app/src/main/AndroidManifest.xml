<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.blueapps.lightspace">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_TASKS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".beacon.BeaconActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LightSpaceActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name="com.philips.lighting.quickstart.PHHomeActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.philips.lighting.quickstart.MyApplicationActivity"
            android:label="@string/txt_selectbridges"></activity>
        <activity
            android:name="com.philips.lighting.quickstart.PHPushlinkActivity"
            android:label="@string/app_name"></activity>
    </application>

</manifest>
